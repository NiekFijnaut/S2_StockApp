@model AccountStockViewModel
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<table class="custom-table">
    <thead>
        <tr>
            <th>Symbol</th>
            <th>Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.AccountStocks)
        {
            <tr>
                <td>@item.Symbol</td>
                <td>@item.Date</td>
            </tr>
        }
    </tbody>
</table>
